<VirtualHost *:80>
    DocumentRoot /var/www/strongholdstudios.se/
    ServerName strongholdstudios.se
    ServerAlias www.strongholdstudios.se
    ErrorLog /var/log/apache2/stronghold-error.log

    <Directory />
        AllowOverride None
        Options -Indexes
    </Directory>
    
    <Directory /var/www/stronghold/.git/>
        Deny From All
    </Directory>

    LogLevel info
    CustomLog /var/log/apache2/stronghold-access.log combined
</VirtualHost>
